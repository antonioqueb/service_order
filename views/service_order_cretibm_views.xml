<odoo>
  <record id="service_order_form_cretibm" model="ir.ui.view">
    <field name="name">service.order.form.cretibm</field>
    <field name="model">service.order</field>
    <field name="inherit_id" ref="service_order.view_service_order_form"/>
    <field name="arch" type="xml">
      <!-- 1) En la lista de líneas de Servicio, justo después de 'packaging_id' -->
      <xpath expr="//page[@string='Líneas de Servicio']//list/field[@name='packaging_id']" position="after">
        <field name="c"/>
        <field name="r"/>
        <field name="e"/>
        <field name="t"/>
        <field name="i"/>
        <field name="b"/>
        <field name="m"/>
      </xpath>
    </field>
  </record>
</odoo>
